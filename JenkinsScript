                    }
                }
                catch(err)
                {
                    echo "Error"
                }
            }
        }
    }
    stage('Log Analysis'){
        dir('../framework'){
            sh '/usr/local/Cellar/python/3.6.4_3/bin/python LogParser.py'
            sh 'cat result.html'
        }
    }
    publishHTML([allowMissing: false, alwaysLinkToLastBuild: false, keepAll: false, reportDir: '/Users/Shared/Jenkins/Home/workspace', reportFiles: 'result.html', reportName: 'InterOperability Report', reportTitles: 'InterOperability Matrix'])
